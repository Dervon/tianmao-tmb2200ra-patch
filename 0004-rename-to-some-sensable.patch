Subject: [PATCH 4/6] rename to some sensable

---
 arch/arm/boot/dts/sun6i-a31s-tmb2200ra.dts | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm/boot/dts/sun6i-a31s-tmb2200ra.dts b/arch/arm/boot/dts/sun6i-a31s-tmb2200ra.dts
index 9350f11..081bb07 100644
--- a/arch/arm/boot/dts/sun6i-a31s-tmb2200ra.dts
+++ b/arch/arm/boot/dts/sun6i-a31s-tmb2200ra.dts
@@ -16,10 +16,10 @@
 	soc@01c00000 {
 		mmc0: mmc@01c0f000 {
 			pinctrl-names = "default";
-			pinctrl-0 = <&mmc0_pins_a>, <&mmc0_cd_pin_primo81>;
+			pinctrl-0 = <&mmc0_pins_a>, <&mmc0_cd_tmb2200ra>;
 			vmmc-supply = <&reg_vcc3v0>;
 			bus-width = <4>;
-			cd-gpios = <&pio 0 4 0>; /* PA8 should be PA4!!! */
+			cd-gpios = <&pio 0 4 GPIO_ACTIVE_HIGH>; /* PA4 */
 			cd-inverted;
 			status = "okay";
 		};
@@ -60,7 +60,7 @@
 		};
 
 		pio: pinctrl@01c20800 {
-			mmc0_cd_pin_primo81: mmc0_cd_pin@0 {
+			mmc0_cd_tmb2200ra: mmc0_cd_pin@0 {
 				allwinner,pins = "PA4";
 				allwinner,function = "gpio_in";
 				allwinner,drive = <0>;
