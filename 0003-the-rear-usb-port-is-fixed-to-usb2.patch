+++ b/arch/arm/boot/dts/sun6i-a31s-tmb2200ra.dts
@@ -33,7 +33,8 @@
 		};
 
 		usbphy: phy@01c19400 {
-            usb1_vbus-supply = <&reg_usb1_vbus>;
+//          usb1_vbus-supply = <&reg_aldo1>;
+            usb2_vbus-supply = <&reg_usb2_vbus>;
 			status = "okay";
 		};
 
@@ -81,9 +82,15 @@
 	};
 };
 
-&reg_usb1_vbus {
+&reg_usb2_vbus {
 	gpio = <&pio 2 27 GPIO_ACTIVE_HIGH>; /* PC27 */
 	status = "okay";
 };
 
